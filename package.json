{
  "name": "@telaclaims-tech/chat",
  "version": "0.0.12",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/chat/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npm run build && vite preview",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:package": "vite build --config vite.config.package.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "publish-pkg": "npm run build:package && npm version patch && git push && npm publish"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.12",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.15",
    "@mui/system": "^5.15.15",
    "@twilio/conversations": "^2.5.0",
    "libphonenumber-js": "^1.10.60",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^9.10.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.11",
    "@types/react": "^17.0.76",
    "@types/react-dom": "^17.0.25",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "terser": "^5.31.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.8.1"
  },
  "engines": {
    "node": ">=16.16.0",
    "npm": ">=8.11.0"
  },
  "volta": {
    "node": "16.16.0"
  }
}
